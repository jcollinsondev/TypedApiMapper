{
  "name": "typed-api-mapper",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "url": "https://github.com/jcollinsondev/TypedApiMapper",
    "type": "git"
  },
  "license": "MIT",
  "author": "Julian Collinson <julian.collinson1998@gmail.com>",
  "scripts": {
    "validate": "kcd-scripts validate",
    "pre-commit": "kcd-scripts pre-commit",
    "commit": "cz",
    "build": "rd /s /q .\\lib\\ && kcd-scripts build --out-dir lib",
    "prerelease": "git checkout main && git pull origin main && yarn install && yarn validate && git add .",
    "release": "standard-version -a",
    "release:tags": "git push --follow-tags origin master",
    "test": "kcd-scripts test --detectOpenHandles --forceExit",
    "typecheck": "kcd-scripts typecheck",
    "lint": "kcd-scripts lint",
    "format": "kcd-scripts format",
    "publish:lib": "yarn build && yarn publish .\\lib\\ --access=public"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "axios": "^1.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "kcd-scripts": "^12.3.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
